<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Clockin.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Clockin"
    Title="Clockin">

    <TabBar x:Name="MainTabBar" Route="tabs" x:FieldModifier="public">
        <ShellContent Title="First" ContentTemplate="{DataTemplate local:MainPage}" />
